{
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "colors": {
    "editor.background": "#1B1917",
    "editor.foldBackground": "#1B191777",
    "editor.foreground": "#CEB7AB",
    "editor.lineHighlightBackground": "#24211F",
    "editor.selectionBackground": "#333333",
    "editorCursor.foreground": "#B4A5C5",
    "editorIndentGuide.background1": "#8f8989",
    "editorLineNumber.activeForeground": "#B4A5C5",
    // "editorLineNumber.foreground": "#CEB7AB",
    "editorRuler.foreground": "#8f8989",
    "statusBar.background": "#211E1C",
    "statusBar.foreground": "#ece7e7",

    "sideBar.border": "#48413c",
    "panel.border": "#48413c",
    "tab.border": "#48413c",
    "statusBar.border": "#48413c",

    "sideBar.background": "#211E1C",
    "titleBar.activeBackground": "#211E1C",
    "tab.inactiveBackground": "#211E1C",
    "textLink.activeForeground": "#1D9CCD",
    "editorLink.activeForeground": "#1D9CCD",
    "list.activeSelectionBackground": "#1D9CCD",
    "editorGutter.addedBackground": "#487a1d88",
    "editorGutter.deletedBackground": "#A50303",
    "editorGutter.modifiedBackground": "#76622288",

    // Intellisense icons
    "symbolIcon.classForeground": "#995f5a",
    "symbolIcon.colorForeground": "#995f5a",
    "symbolIcon.constructorForeground": "#995f5a",
    "symbolIcon.enumeratorForeground": "#995f5a",
    "symbolIcon.interfaceForeground": "#995f5a",
    "symbolIcon.moduleForeground": "#995f5a",
    "symbolIcon.namespaceForeground": "#995f5a",
    "symbolIcon.typeParameterForeground": "#995f5a",
    "symbolIcon.packageForeground": "#995f5a",
    "symbolIcon.objectForeground": "#995f5a",
    "symbolIcon.eventForeground": "#c26b85",
    "symbolIcon.fileForeground": "#c26b85",
    "symbolIcon.folderForeground": "#c26b85",
    "symbolIcon.keywordForeground": "#c26b85",
    "symbolIcon.operatorForeground": "#c26b85",
    "symbolIcon.snippetForeground": "#c26b85",
    "symbolIcon.structForeground": "#c26b85",
    "symbolIcon.arrayForeground": "#b07c62",
    "symbolIcon.booleanForeground": "#5f4031",
    "symbolIcon.methodForeground": "#5f4031",
    "symbolIcon.propertyForeground": "#5f4031",
    "symbolIcon.fieldForeground": "#5f4031",
    "symbolIcon.functionForeground": "#5f4031",
    "symbolIcon.textForeground": "#a2a8b8",
    "symbolIcon.variableForeground": "#a2a8b8",
    "symbolIcon.stringForeground": "#b66c9e",
    "symbolIcon.referenceForeground": "#b66c9e",
    "symbolIcon.unitForeground": "#b66c9e",
    "symbolIcon.constantForeground": "#b66c9e",
    "symbolIcon.enumeratorMemberForeground": "#b66c9e",
    "symbolIcon.keyForeground": "#b66c9e",
    "symbolIcon.nullForeground": "#b66c9e",
    "symbolIcon.numberForeground": "#b66c9e"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#5c5550"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#e08f71" // c87c61
      }
    },
    {
      "scope": [
        "meta.function-call entity.name.function",
        "meta.function-call",
        "variable.function",
        "entity.name.function.support.go",
        "entity.name.type.cs",
        "meta.type.annotation entity.name.type"
      ],
      "settings": {
        "foreground": "#CEB7AB",
        "fontStyle": "none"
      }
    },
    {
      "scope": ["keyword", "keyword.control", "keyword.operator", "storage.modifier"],
      "settings": {
        "foreground": "#EBE6E6",
      }
    },
    {
      "scope": ["storage.type", "support.class.component.tsx", "meta.tag.custom.start.html entity.name.tag.html"],
      "settings": {
        "foreground": "#EBE6E6",
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#d37291",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#d37291",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.heading.markdown", "support.type.property-name.json", "storage.type.class.jsdoc"],
      "settings": {
        "foreground": "#d783bb",
      }
    },
    {
      "scope": [],
      "settings": {
        "foreground": "#CEB7AB"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#A50303",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.underline"],
      "settings": {
        "foreground": "#CEB7AB",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "meta.diff",
      "settings": {
        "foreground": "#38551E"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#CEB7AB",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["constant.language", "constant.character"],
      "settings": {
        "foreground": "#CEB7AB"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#38551E"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#554717"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#A50303"
      }
    },
    {
      "scope": "markup.punctuation",
      "settings": {
        "foreground": "#1E5571"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#316BCD"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#CD3131"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#800080"
      }
    }
  ]
}
