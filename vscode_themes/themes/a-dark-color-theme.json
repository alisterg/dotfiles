  },
  "tokenColors": [
    {
      "scope": [
        "meta.embedded.line.ts",
        "source.groovy.embedded",
        "string meta.image.inline.markdown"
      ],
      "settings": {
        "foreground": "#b6cccc"
      }
    },
    {
      "scope": [
        "string.json.comments",
        "support.type.property-name.json",
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#b6cccc"
      }
    },
    {
      "scope": "punctuation.definition.comment",
      "settings": {
        "foreground": "#28bdae99"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "scope": "comment.block.documentation",
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#28bdae"
      }
    },
    {
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "punctuation.definition.string.template.begin.ts",
        "punctuation.definition.string.template.end.ts"
      ],
      "settings": {
        "foreground": "#28bdae99"
      }
    },

    
    {
      "scope": ["keyword.control", "keyword.other"],
      "settings": {
        "foreground": "#f0509d"
      }
    },
    {
      "scope": ["storage.modifier", "storage.type"],
      "settings": {
        "foreground": "#57a2e8" // "#393f3f",
        // "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.other.var.cs", "storage.type"],
      "settings": {
        "foreground": "#b6cccc"
        // "fontStyle": "bold"
      }
    },


    {
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.other.using",
        "keyword.other.namespace",
        "keyword.control.from",
        "keyword.other.use.php"
      ],
      "settings": {
        "foreground": "#d96634"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#f76ea9"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#92b300"
      }
    },
    {
      "scope": "variable.language.this",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        // "variable.parameter",
        "punctuation.definition.variable.php"
      ],
      "settings": {
        "foreground": "#92b300"
      }
    },
    {
      "scope": ["entity.name.type"],
      "settings": {
        "foreground": "#9897da"
      }
    },
    // {
    //   "name": "function definitions",
    //   "scope": [
    //     "meta.object.member.ts entity.name.function.ts",
    //     "meta.method.declaration.ts entity.name.function.ts",
    //     "meta.definition.function",
    //     "meta.definition.variable entity.name.function"
    //   ],
    //   "settings": {
    //     "foreground": "#b6cccc"
    //   }
    // },
    {
      "name": "overrides for function call",
      "scope": [
        "meta.method.declaration.ts meta.function-call.ts entity.name.function.ts",
        "meta.function-call.ts entity.name.function.ts",
        "meta.method-call",
        "entity.name.function.cs",
        "meta.function.php",
        "meta.function-call.tsx entity.name.function.tsx",
        "entity.name.variable.parameter.cs",
        "storage.type.function",
        "variable.parameter",
        "entity.name.function.js",


        "meta.object.member.ts entity.name.function.ts",
        "meta.method.declaration.ts entity.name.function.ts",
        "meta.definition.function",
        "meta.definition.variable entity.name.function"
      ],
      "settings": {
        "foreground": "#57a2e8"
      }
    },
    {
      "name": "tsx overrides",
      "scope": [
        "meta.embedded.expression.tsx",
        "punctuation.section.embedded.begin.tsx",
        "punctuation.section.embedded.end.tsx"
      ],
      "settings": {
        "foreground": "#b6cccc"
      }
    },
    {
      "scope": [
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end",
        "markup.heading.markdown"
      ],
      "settings": {
        "foreground": "#d96634"
      }
    },
    {
      "scope": "meta.paragraph.markdown",
      "settings": {
        "foreground": "#b6cccc"
      }
    },
    {
      "scope": ["meta.preprocessor"],
      "settings": {
        "foreground": "#d96634" //cb696d
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#9897da"
      }
    },
    {
      "scope": ["support.type.primitive", "keyword.type"],
      "settings": {
        "foreground": "#9897da"
      }
    },
    {
      "scope": "punctuation.separator.continuation",
      "settings": {
        "foreground": "#d96634"
      }
    },
    {
      "scope": ["support.constant", "support.variable"],
      "settings": {}
    },
    {
      "scope": "support.type.exception",
      "settings": {
        "foreground": "#d96634"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#d96634"
      }
    },
    {
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#9897da",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#d96634",
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#d96634",
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "scope": "markup.list",
      "settings": {
        "foreground": "#b6cccc"
      }
    },
    {
      "scope": ["markup.bold", "markup.italic"],
      "settings": {
        "foreground": "#f0509d"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#b6cccc",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#9897da",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "punctuation.definition.link.title.begin.markdown",
        "punctuation.definition.link.title.end.markdown"
      ],
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "scope": "meta.link.inline.markdown",
      "settings": {
        "foreground": "#f76ea9"
      }
    },
    {
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "#9897da",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "entity.name.tag.html",
        "entity.name.tag.tsx",
        "entity.name.tag.xml"
      ],
      "settings": {
        "foreground": "#9897da"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#B267E6"
      }
    }
  ]
}
