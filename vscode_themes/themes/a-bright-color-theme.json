    "pickerGroup.foreground": "#000008",
    "ports.iconRunningProcessForeground": "#369432",
    "progressBar.background": "#047aa6",
    "quickInputList.focusBackground": "#d3368288",
    // "quickInput.background": "#e0e4e5cc",
    "selection.background": "#f3f0f099",
    "sideBar.foreground": "#000008",
    "sideBar.background": "#e0e4e5",
    "sideBarTitle.foreground": "#000008",
    "sideBarSectionHeader.background": "#e0e4e5",
    "statusBar.background": "#e0e4e5",
    "statusBar.debuggingBackground": "#e0e4e5",
    "statusBar.foreground": "#000008",
    "statusBar.noFolderBackground": "#e0e4e5",
    "statusBarItem.prominentBackground": "#003847",
    "statusBarItem.prominentHoverBackground": "#003847",
    "statusBarItem.remoteBackground": "#0d958999",
    "tab.activeBackground": "#f1f2f5",
    // "tab.activeForeground": "#6c6f6f",
    // "tab.border": "#003847",
    "tab.inactiveBackground": "#e0e4e5",
    "tab.inactiveForeground": "#000008",
    "tab.lastPinnedBorder": "#0d958944",
    "terminal.ansiBlack": "#073642",
    "terminal.ansiBlue": "#6850A1",
    "terminal.ansiBrightBlack": "#002b36",
    "terminal.ansiBrightBlue": "#000008",
    "terminal.ansiBrightCyan": "#000008",
    "terminal.ansiBrightGreen": "#849090",
    "terminal.ansiBrightMagenta": "#6850A1",
    "terminal.ansiBrightRed": "#cb4b16",
    "terminal.ansiBrightWhite": "#fdf6e3",
    "terminal.ansiBrightYellow": "#657b83",
    "terminal.ansiCyan": "#0d9589",
    "terminal.ansiGreen": "#859900",
    "terminal.ansiMagenta": "#d5267b",
    "terminal.ansiRed": "#cb4b16",
    "terminal.ansiWhite": "#eee8d5",
    "terminal.ansiYellow": "#b58900",
    "titleBar.activeBackground": "#e0e4e5",
    "editorGutter.addedBackground": "#1a2f18",
    "editorGutter.deletedBackground": "#772828",
    "editorGutter.modifiedBackground": "#18576f",
    "editorHoverWidget.border": "#b7b7b7",
    "notifications.background": "#e0e4e5",
    "panel.background": "#e0e4e5",
    // "editorBracketHighlight.foreground1": "#000008bb",
    "editorBracketHighlight.foreground2": "#000008",
    "editorBracketHighlight.foreground3": "#000008",
    "editorBracketHighlight.foreground1": "#000008",
    "editorBracketHighlight.foreground4": "#000008",
    // icons
    "symbolIcon.arrayForeground": "#1777bc",
    "symbolIcon.booleanForeground": "#1777bc",
    "symbolIcon.classForeground": "#d5267b",
    "symbolIcon.colorForeground": "#d5267b",
    "symbolIcon.constantForeground": "#ef9f76",
    "symbolIcon.constructorForeground": "#d5267b",
    "symbolIcon.enumeratorForeground": "#d5267b",
    "symbolIcon.enumeratorMemberForeground": "#e5c890",
    "symbolIcon.eventForeground": "#f4b8e4",
    "symbolIcon.fieldForeground": "#1777bc",
    "symbolIcon.fileForeground": "#85c1dc",
    "symbolIcon.folderForeground": "#85c1dc",
    "symbolIcon.functionForeground": "#1777bc",
    "symbolIcon.interfaceForeground": "#d5267b",
    "symbolIcon.keyForeground": "#0d9589",
    "symbolIcon.keywordForeground": "#d5267b",
    "symbolIcon.methodForeground": "#1777bc",
    "symbolIcon.moduleForeground": "#d5267b",
    "symbolIcon.namespaceForeground": "#d5267b",
    "symbolIcon.nullForeground": "#1777bc",
    "symbolIcon.numberForeground": "#1777bc",
    "symbolIcon.objectForeground": "#d5267b",
    "symbolIcon.operatorForeground": "#0d9589",
    "symbolIcon.packageForeground": "#d5267b",
    "symbolIcon.propertyForeground": "#1777bc",
    "symbolIcon.referenceForeground": "#e5c890",
    "symbolIcon.snippetForeground": "#eebebe",
    "symbolIcon.stringForeground": "#0d9589",
    "symbolIcon.structForeground": "#EE759D",
    "symbolIcon.textForeground": "#d9dbe1",
    "symbolIcon.typeParameterForeground": "#d5267b",
    "symbolIcon.unitForeground": "#e5c890",
    "symbolIcon.variableForeground": "#000008"
  },
  "tokenColors": [
    {
      "scope": [
        "meta.embedded.line.ts",
        "source.groovy.embedded",
        "string meta.image.inline.markdown"
      ],
      "settings": {
        "foreground": "#000008"
      }
    },
    {
      "scope": [
        "string.json.comments",
        "support.type.property-name.json",
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#000008"
      }
    },
    {
      "scope": "punctuation.definition.comment",
      "settings": {
        "foreground": "#0d958999"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#849090"
      }
    },
    {
      "scope": "comment.block.documentation",
      "settings": {
        "foreground": "#849090"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#0d9589"
      }
    },
    {
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "punctuation.definition.string.template.begin.ts",
        "punctuation.definition.string.template.end.ts"
      ],
      "settings": {
        "foreground": "#0d958999"
      }
    },
    {
      "scope": ["keyword.control", "keyword.other"],
      "settings": {
        "foreground": "#d5267b"
      }
    },
    {
      "scope": ["storage.modifier", "storage.type"],
      "settings": {
        "foreground": "#1777bc", // "#000008",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.other.var.cs", "storage.type", "keyword.var.go", "keyword.function.go"],
      "settings": {
        "foreground": "#000008",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.other.using",
        "keyword.other.namespace",
        "keyword.control.from",
        "keyword.other.use.php"
      ],
      "settings": {
        "foreground": "#6650A0",
        "fontStyle": "none"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#f76ea9"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#528f0c"
      }
    },
    {
      "scope": "variable.language.this",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        // "variable.parameter",
        "punctuation.definition.variable.php"
      ],
      "settings": {
        "foreground": "#528f0c"
      }
    },
    {
      "scope": ["entity.name.type"],
      "settings": {
        "foreground": "#6850A1"
      }
    },
    {
      "name": "overrides for function call",
      "scope": [
        "meta.object.member.ts entity.name.function.ts",
        "meta.method.declaration.ts entity.name.function.ts",
        "meta.definition.function",
        "meta.definition.variable entity.name.function",
        "meta.method.declaration.ts meta.function-call.ts entity.name.function.ts",
        "meta.function-call.ts entity.name.function.ts",
        "meta.method-call",
        "entity.name.function",
        "meta.function.php",
        "meta.function-call.tsx entity.name.function.tsx",
        "entity.name.variable.parameter.cs",
        "storage.type.function",
        "variable.parameter",
        "support.function.go"
      ],
      "settings": {
        "foreground": "#1777bc"
      }
    },
    {
      "name": "tsx overrides",
      "scope": [
        "meta.embedded.expression.tsx",
        "punctuation.section.embedded.begin.tsx",
        "punctuation.section.embedded.end.tsx"
      ],
      "settings": {
        "foreground": "#000008"
      }
    },
    {
      "scope": [
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end",
        "markup.heading.markdown"
      ],
      "settings": {
        "foreground": "#6650A0"
      }
    },
    {
      "scope": "meta.paragraph.markdown",
      "settings": {
        "foreground": "#000008"
      }
    },
    {
      "scope": ["meta.preprocessor"],
      "settings": {
        "foreground": "#cb4b16" //cb696d
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#6850A1"
      }
    },
    {
      "scope": ["support.type.primitive", "keyword.type"],
      "settings": {
        "foreground": "#6850A1"
      }
    },
    {
      "scope": "punctuation.separator.continuation",
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "scope": ["support.constant", "support.variable"],
      "settings": {}
    },
    {
      "scope": "support.type.exception",
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#6850A1",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#cb4b16",
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#cb4b16",
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "scope": "markup.list",
      "settings": {
        "foreground": "#000008"
      }
    },
    {
      "scope": ["markup.bold", "markup.italic"],
      "settings": {
        "foreground": "#d5267b"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#000008",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#0d9589"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#6850A1",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "punctuation.definition.link.title.begin.markdown",
        "punctuation.definition.link.title.end.markdown"
      ],
      "settings": {
        "foreground": "#849090"
      }
    },
    {
      "scope": "meta.link.inline.markdown",
      "settings": {
        "foreground": "#f76ea9"
      }
    },
    {
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "#6850A1",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "entity.name.tag.html",
        "entity.name.tag.tsx",
        "entity.name.tag.xml"
      ],
      "settings": {
        "foreground": "#6850A1"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#B267E6"
      }
    }
  ]
}
