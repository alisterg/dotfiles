{
  "name": "modus-vivendi-code",
  "tokenColors": [
    {
      "name": "Global settings",
      "settings": {
        "background": "#000000",
        "foreground": "#00BDFA"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#72A4FF"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace",
        "punctuation.definition.string.end.json.comments",
        "punctuation.definition.string.begin.json.comments"
      ],
      "settings": {
        "foreground": "#cacaca99"
      }
    },
    {
      "name": "String Escape",
      "scope": "constant.character.escape, text.html constant.character.entity.named, punctuation.definition.entity.html",
      "settings": {
        "foreground": "#abf6ff"
      }
    },
    {
      "name": "simple types",
      "scope": [
        "constant.language.boolean",
        "constant.numeric",
        "support.type.primitive",
        "constant.language.undefined",
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#6ae4b9"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "modifier",
        "support.type.object",
        "constant.language",
        "keyword.control.export",
        "keyword.control.import",
        "keyword.control.from"
      ],
      "settings": {
        "foreground": "#B6A0FF"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#cacaca"
      }
    },
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#F78FE7"
      }
    },
    {
      "scope": "variable.language.this",
      "settings": {
        "foreground": "#caa6df"
      }
    },
    {
      "name": "function definitions",
      "scope": [
        "meta.object.member.ts entity.name.function.ts",
        "meta.method.declaration.ts entity.name.function.ts",
        "meta.definition.method.js entity.name.function.js",
        "meta.definition.function",
        "meta.definition.variable entity.name.function"
      ],
      "settings": {
        "foreground": "#feacd0", // DCDCAA
        "fontStyle": "underline"
      }
    },
    {
      "scope": "entity.name.function.cs",
      "settings": {
        "foreground": "#feacd0"
      }
    },
    {
      "name": "overrides for function call",
      "scope": [
        "meta.method.declaration.ts meta.function-call.ts entity.name.function.ts",
        "meta.function-call.ts entity.name.function.ts"
      ],
      "settings": {
        "foreground": "#cacaca"
      }
    },
    {
      "name": "Storage",
      "scope": "storage.type, storage.modifier",
      "settings": {
        "foreground": "#B6A0FF"
      }
    },
    {
      "name": "Modules",
      "scope": "support.module, support.node",
      "settings": {
        "foreground": "#cacaca",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type",
      "scope": "support.type",
      "settings": {
        "foreground": "#cacaca"
      }
    },
    {
      "name": "Type",
      "scope": "entity.name.type, entity.other.inherited-class",
      "settings": {
        "foreground": "#00d3d0"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#419F7C",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "comment.block.documentation",
      "settings": {
        "foreground": "#419F7C"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.type.class",
        "entity.name.type.interface",
        "entity.name.type.alias.ts"
      ],
      "settings": {
        "foreground": "#cacaca",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Class variable",
      "scope": "variable.object.property, meta.field.declaration entity.name.function",
      "settings": {
        "foreground": "#cacaca"
      }
    },
    {
      "name": "Class method",
      "scope": "meta.definition.method entity.name.function",
      "settings": {
        "foreground": "#cacaca"
      }
    },
    {
      "name": "Template expression",
      "scope": "template.expression.begin, template.expression.end, punctuation.definition.template-expression.begin, punctuation.definition.template-expression.end",
      "settings": {
        "foreground": "#B6A0FF"
      }
    },
    {
      "scope": "source.ts.embedded.html",
      "settings": {
        "foreground": "#4ae2f0"
      }
    },
    {
      "name": "Reset embedded/template expression colors",
      "scope": "meta.embedded, source.groovy.embedded, meta.template.expression",
      "settings": {
        "foreground": "#00BDFA"
      }
    },
    {
      "name": "YAML key",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#cacaca"
      }
    },
    {
      "name": "JSON key",
      "scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
      "settings": {
        "foreground": "#cacaca"
      }
    },
    {
      "name": "JSON constant",
      "scope": "constant.language.json",
      "settings": {
        "foreground": "#B6A0FF"
      }
    },
    {
      "name": "CSS class",
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#B6A0FF"
      }
    },
    {
      "name": "CSS ID",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#72A4FF"
      }
    },
    {
      "name": "CSS tag",
      "scope": "source.css entity.name.tag",
      "settings": {
        "foreground": "#cacaca"
      }
    },
    {
      "name": "HTML tag outer",
      "scope": "meta.tag, punctuation.definition.tag",
      "settings": {
        "foreground": "#B6A0FF"
      }
    },
    {
      "name": "HTML tag inner",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#b6a0ff"
      }
    },
    {
      "name": "HTML tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#F78FE7"
      }
    },
    {
      "name": "Markdown heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#B6A0FF"
      }
    },
    {
      "name": "Markdown link text",
      "scope": "text.html.markdown meta.link.inline, meta.link.reference",
      "settings": {
        "foreground": "#cacaca"
      }
    },
    {
      "name": "Markdown list item",
      "scope": "text.html.markdown beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#B6A0FF"
      }
    },
    {
      "name": "Markdown italic",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#cacaca",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#cacaca",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold italic",
      "scope": "markup.bold markup.italic, markup.italic markup.bold",
      "settings": {
        "foreground": "#cacaca",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Markdown code block",
      "scope": "markup.fenced_code.block.markdown punctuation.definition.markdown",
      "settings": {
        "foreground": "#72A4FF"
      }
    },
    {
      "name": "Markdown inline code",
      "scope": "markup.inline.raw.string.markdown",
      "settings": {
        "foreground": "#72A4FF"
      }
    },
    {
      "name": "INI property name",
      "scope": "keyword.other.definition.ini",
      "settings": {
        "foreground": "#cacaca"
      }
    },
    {
      "name": "INI section title",
      "scope": "entity.name.section.group-title.ini",
      "settings": {
        "foreground": "#B6A0FF"
      }
    },
    {
      "name": "C# class",
      "scope": "source.cs meta.class.identifier storage.type",
      "settings": {
        "foreground": "#cacaca",
        "fontStyle": "underline"
      }
    },
    {
      "name": "C# class method",
      "scope": "source.cs meta.method.identifier entity.name.function",
      "settings": {
        "foreground": "#cacaca"
      }
    },
    {
      "name": "C# function call",
      "scope": "source.cs meta.method-call meta.method, source.cs entity.name.function",
      "settings": {
        "foreground": "#F78FE7"
      }
    },
    {
      "name": "C# type",
      "scope": "source.cs storage.type",
      "settings": {
        "foreground": "#cacaca"
      }
    },
    {
      "name": "C# return type",
      "scope": "source.cs meta.method.return-type",
      "settings": {
        "foreground": "#cacaca"
      }
    },
    {
      "name": "C# namespace",
      "scope": "source.cs entity.name.type.namespace",
      "settings": {
        "foreground": "#00BDFA"
      }
    }
  ],
  "colors": {
    // Controls / misc
    "focusBorder": "#B6A0FF",
    "foreground": "#c3c5c5", // 00BDFA
    "button.background": "#B6A0FF",
    "button.foreground": "#cacaca",
    "dropdown.background": "#000000",
    "input.background": "#000000",
    "inputOption.activeBorder": "#B6A0FF",
    // File explorer
    "list.activeSelectionBackground": "#B6A0FF80",
    "list.activeSelectionForeground": "#cacaca",
    "list.dropBackground": "#B6A0FF80",
    "list.focusBackground": "#B6A0FF80",
    "list.focusForeground": "#cacaca",
    "list.highlightForeground": "#B6A0FF",
    "list.hoverBackground": "#cacaca1a",
    "list.inactiveSelectionBackground": "#cacaca33",
    "list.errorForeground": "#bb726e",
    "list.warningForeground": "#c3c5c5",
    "gitDecoration.modifiedResourceForeground": "#bdb895",
    "gitDecoration.untrackedResourceForeground": "#00d9ff",
    // Activity bar
    "activityBar.background": "#000000",
    "activityBar.dropBackground": "#B6A0FF80",
    "activityBarBadge.background": "#B6A0FF",
    "activityBarBadge.foreground": "#cacaca",
    "badge.background": "#B6A0FF",
    "badge.foreground": "#cacaca",
    // Sidebar
    "sideBar.background": "#000000",
    "sideBarSectionHeader.background": "#000000",
    //
    "editorGroup.dropBackground": "#B6A0FF80",
    "editorGroup.focusedEmptyBorder": "#B6A0FF",
    "editorGroupHeader.tabsBackground": "#000000",
    // Tabs
    "tab.border": "#00000033",
    "tab.inactiveBackground": "#000000",
    "tab.inactiveModifiedBorder": "#6d6099",
    "tab.unfocusedActiveModifiedBorder": "#9180cc",
    "tab.unfocusedInactiveModifiedBorder": "#6d6099",
    "tab.activeBackground": "#b6a0ff77",
    // Editor
    "editor.background": "#000000",
    "editor.foreground": "#cacaca", // cacaca e5e8e8
    "editorLineNumber.foreground": "#cacaca4d",
    "editorLineNumber.activeForeground": "#B6A0FF",
    "editorCursor.foreground": "#cacaca",
    "editor.selectionBackground": "#3C3C3C",
    "editor.lineHighlightBorder": "#b6a0ff77",
    "editor.rangeHighlightBackground": "#cacaca0d",
    // Minimap
    "minimapSlider.background": "#b6a0ff45",
    "minimapSlider.hoverBackground": "#b6a0ff45",
    // Misc
    "window.activeBorder": "#b6a0ff77",
    "window.inactiveBorder": "#b6a0ff77",
    "contrastBorder": "#cacaca1a",
    // command center / F1 menu
    "quickInput.background": "#282438",
    // Todo below
    "sideBar.border": "#000000",
    "statusBar.border": "#000000",
    "sideBarSectionHeader.border": "#000000",
    "editorLink.activeForeground": "#3C3C3C",
    "editorWidget.background": "#000000",
    "editorHoverWidget.background": "#000000",
    "editorMarkerNavigation.background": "#000000",
    "peekView.border": "#B6A0FF",
    "peekViewEditor.background": "#000000",
    "peekViewResult.background": "#000000",
    "peekViewTitle.background": "#000000",
    "panel.background": "#000000",
    "panel.border": "#cacaca1a",
    "panelTitle.activeBorder": "#00BDFA80",
    "panelTitle.inactiveForeground": "#00BDFA80",
    "statusBar.background": "#000000",
    "statusBar.debuggingBackground": "#B6A0FF",
    "statusBar.debuggingForeground": "#cacaca",
    "statusBar.noFolderBackground": "#000000",
    "statusBarItem.activeBackground": "#B6A0FF80",
    "statusBarItem.hoverBackground": "#cacaca1a",
    "statusBarItem.remoteBackground": "#B6A0FF",
    "statusBarItem.remoteForeground": "#cacaca",
    "titleBar.activeBackground": "#000000",
    "pickerGroup.border": "#cacaca1a",
    "debugToolBar.background": "#000000",
    "selection.background": "#B6A0FF",
    "editorGutter.modifiedBackground": "#72A4FF"
  }
}
