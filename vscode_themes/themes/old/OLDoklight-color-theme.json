  "tokenColors": [
    {
      "scope": [
        // "meta.embedded",
        "source.groovy.embedded",
        "string meta.image.inline.markdown"
      ],
      "settings": {
        "foreground": "#b25b29"
      }
    },
    {
      "scope": "string.json.comments",
      "settings": {
        "foreground": "#302f2e"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#6C7986"
      }
    },
    {
      "scope": "comment.block.documentation",
      "settings": {
        "foreground": "#6C7986"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#44780d"
      }
    },
    {
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "punctuation.definition.string.template.begin.ts",
        "punctuation.definition.string.template.end.ts"
      ],
      "settings": {
        "foreground": "#44780d99"
      }
    },
    {
      "scope": ["keyword.control", "storage.modifier"],
      "settings": {
        "foreground": "#0e9191",
      }
    },
    {
      "scope": [
        "keyword.control.import",
        "keyword.other.using",
        "keyword.other.namespace",
        "keyword.control.from"
      ],
      "settings": {
        "foreground": "#b25b29"
      }
    },
    {
      "scope": ["storage.type", "keyword.other"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#b25b29"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#a94448"
      }
    },
    {
      "scope": "variable.language.this",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.type"],
      "settings": {
        "fontStyle": "bold",
      }
    },
    {
      "name": "function definitions",
      "scope": [
        "meta.object.member.ts entity.name.function.ts",
        "meta.method.declaration.ts entity.name.function.ts",
        "meta.definition.function",
        "meta.definition.variable entity.name.function",
        "entity.name.function.cs",
        "meta.function.php"
      ],
      "settings": {
        "foreground": "#126d9e"
      }
    },
    {
      "name": "overrides for function call",
      "scope": [
        "meta.method.declaration.ts meta.function-call.ts entity.name.function.ts",
        "meta.function-call.ts entity.name.function.ts",
        "meta.method-call",
        "meta.function-call.tsx entity.name.function.tsx"
      ],
      "settings": {
        "foreground": "#302f2e" // 2387B5
      }
    },
    {
      "scope": ["punctuation", "meta.brace", "keyword.operator"],
      "settings": {
        "foreground": "#926a90"
      }
    },
    {
      "name": "tsx overrides",
      "scope": [
        "meta.embedded.expression.tsx",
        "punctuation.section.embedded.begin.tsx",
        "punctuation.section.embedded.end.tsx"
      ],
      "settings": {
        "foreground": "#302f2e"
      }
    },
    {
      "scope": [
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end",
        "markup.heading.markdown"
      ],
      "settings": {
        "foreground": "#0e9191"
      }
    },
    {
      "scope": "meta.paragraph.markdown",
      "settings": {
        "foreground": "#302f2e"
      }
    },
    {
      "scope": ["constant.language", "meta.preprocessor"],
      "settings": {
        "foreground": "#a94448" //cb696d
      }
    },
    {
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#a94448"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#268BD2"
      }
    },
    {
      "scope": ["support.type.primitive", "keyword.type"],
      "settings": {
        "foreground": "#8247c6"
      }
    },
    {
      "scope": "punctuation.separator.continuation",
      "settings": {
        "foreground": "#DC322F"
      }
    },
    {
      "scope": ["support.constant", "support.variable"],
      "settings": {}
    },
    {
      "scope": "support.type.exception",
      "settings": {
        "foreground": "#CB4B16"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#DC322F"
      }
    },
    {
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#268BD2",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#DC322F",
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#CB4B16",
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "scope": "markup.list",
      "settings": {
        "foreground": "#302f2e"
      }
    },
    {
      "scope": ["markup.bold", "markup.italic"],
      "settings": {
        "foreground": "#D33682"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#302f2e",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#44780d"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#268BD2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "punctuation.definition.link.title.begin.markdown",
        "punctuation.definition.link.title.end.markdown",
        "string.other.link.title.markdown"
      ],
      "settings": {
        "foreground": "#302f2e"
      }
    },
    {
      "scope": "meta.link.inline.markdown",
      "settings": {
        "foreground": "#b25b29"
      }
    },
    {
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "#268BD2",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "entity.name.tag.html",
        "entity.name.tag.tsx",
        "entity.name.tag.xml"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#B267E6"
      }
    }
  ]
}
