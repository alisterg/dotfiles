{
  "name": "modus-light",
  "tokenColors": [
    {
      "name": "Global settings",
      "settings": {
        "background": "#f7f7f7",
        "foreground": "#009cd0"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#3272e8"
      }
    },
    {
      "name": "Brackets",
      "scope": "punctuation.brackets.round, meta.brace.round.js",
      "settings": {
        "foreground": "#171717"
      }
    },
    {
      "name": "Curly Brackets",
      "scope": "punctuation.brackets.curly, meta.brace.curly.js, meta.property-list.css",
      "settings": {
        "foreground": "#171717"
      }
    },
    {
      "name": "Square Brackets",
      "scope": "punctuation.brackets.square, meta.brace.square.js, meta.selector.css",
      "settings": {
        "foreground": "#171717"
      }
    },
    {
      "name": "String Escape",
      "scope": "constant.character.escape, text.html constant.character.entity.named, punctuation.definition.entity.html",
      "settings": {
        "foreground": "#abf6ff"
      }
    },
    {
      "name": "simple types",
      "scope": [
        "constant.language.boolean",
        "constant.numeric",
        "support.type.primitive",
        "constant.language.undefined",
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#2e9974",
        "fontStyle": "none"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword, modifier, support.type.object, constant.language",
        "keyword.control.import",
        "keyword.control.export"
      ],
      "settings": {
        "foreground": "#6650A0"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#171717",
        "fontStyle": "none"
      }
    },
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#009CD0"
      }
    },
    {
      "scope": "variable.language.this",
      "settings": {
        "foreground": "#8268cc"
      }
    },
    {
      "name": "function definitions",
      "scope": [
        "meta.object.member.ts entity.name.function.ts",
        "meta.method.declaration.ts entity.name.function.ts",
        "meta.definition.method.js entity.name.function.js",
        "meta.definition.function",
        "meta.definition.variable entity.name.function"
      ],
      "settings": {
        "foreground": "#b958ab", // purple: a668cc; pink: ab597c; brightpink: b958ab
        "fontStyle": "underline"
      }
    },
    {
      "scope": "entity.name.function.cs",
      "settings": {
        "foreground": "#b958ab"
      }
    },
    {
      "name": "overrides for function call",
      "scope": [
        "meta.method.declaration.ts meta.function-call.ts entity.name.function.ts",
        "meta.function-call.ts entity.name.function.ts"
      ],
      "settings": {
        "foreground": "#171717"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Modules",
      "scope": "support.module, support.node",
      "settings": {
        "foreground": "#171717",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type",
      "scope": "support.type",
      "settings": {
        "foreground": "#171717"
      }
    },
    {
      "name": "Type",
      "scope": "entity.name.type, entity.other.inherited-class",
      "settings": {
        "foreground": "#009d9b"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace",
        "punctuation.definition.string.end.json.comments",
        "punctuation.definition.string.begin.json.comments"
      ],
      "settings": {
        "foreground": "#17171777"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#419F7C",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": "comment.block.documentation",
      "settings": {
        "foreground": "#419F7C"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.type.class",
        "entity.name.type.interface",
        "entity.name.type.alias.ts"
      ],
      "settings": {
        "foreground": "#171717",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Class variable",
      "scope": "variable.object.property, meta.field.declaration entity.name.function",
      "settings": {
        "foreground": "#171717"
      }
    },
    {
      "name": "Class method",
      "scope": "meta.definition.method entity.name.function",
      "settings": {
        "foreground": "#171717"
      }
    },
    {
      "name": "Template expression",
      "scope": "template.expression.begin, template.expression.end, punctuation.definition.template-expression.begin, punctuation.definition.template-expression.end",
      "settings": {
        "foreground": "#2a2a2a"
      }
    },
    {
      "scope": "source.ts.embedded.html",
      "settings": {
        "foreground": "#009cd0"
      }
    },
    {
      "name": "Reset embedded/template expression colors",
      "scope": "meta.embedded, source.groovy.embedded, meta.template.expression",
      "settings": {
        "foreground": "#6650A0"
      }
    },
    {
      "name": "YAML key",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#171717"
      }
    },
    {
      "name": "JSON key",
      "scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
      "settings": {
        "foreground": "#171717"
      }
    },
    {
      "name": "JSON constant",
      "scope": "constant.language.json",
      "settings": {
        "foreground": "#2a2a2a"
      }
    },
    {
      "name": "CSS class",
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#2a2a2a"
      }
    },
    {
      "name": "CSS ID",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#3272e8"
      }
    },
    {
      "name": "CSS tag",
      "scope": "source.css entity.name.tag",
      "settings": {
        "foreground": "#171717"
      }
    },
    {
      "name": "HTML tag outer",
      "scope": "meta.tag, punctuation.definition.tag",
      "settings": {
        "foreground": "#a2a2a2"
      }
    },
    {
      "name": "HTML tag inner",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#2a2a2a"
      }
    },
    {
      "name": "HTML tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#b958ab"
      }
    },
    {
      "name": "Markdown heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#ab597c",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown link text",
      "scope": "text.html.markdown meta.link.inline, meta.link.reference",
      "settings": {
        "foreground": "#171717"
      }
    },
    {
      "name": "Markdown list item",
      "scope": "text.html.markdown beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#2a2a2a"
      }
    },
    {
      "name": "Markdown italic",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#171717",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#171717",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold italic",
      "scope": "markup.bold markup.italic, markup.italic markup.bold",
      "settings": {
        "foreground": "#171717",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Markdown code block",
      "scope": "markup.fenced_code.block.markdown punctuation.definition.markdown",
      "settings": {
        "foreground": "#3272e8"
      }
    },
    {
      "name": "Markdown inline code",
      "scope": "markup.inline.raw.string.markdown",
      "settings": {
        "foreground": "#3272e8"
      }
    },
    {
      "name": "INI property name",
      "scope": "keyword.other.definition.ini",
      "settings": {
        "foreground": "#171717"
      }
    },
    {
      "name": "INI section title",
      "scope": "entity.name.section.group-title.ini",
      "settings": {
        "foreground": "#2a2a2a"
      }
    },
    {
      "name": "C# class",
      "scope": "source.cs meta.class.identifier storage.type",
      "settings": {
        "foreground": "#171717",
        "fontStyle": "underline"
      }
    },
    {
      "name": "C# class method",
      "scope": "source.cs meta.method.identifier entity.name.function",
      "settings": {
        "foreground": "#171717"
      }
    },
    {
      "name": "C# function call",
      "scope": "source.cs meta.method-call meta.method, source.cs entity.name.function",
      "settings": {
        "foreground": "#b958ab"
      }
    },
    {
      "name": "C# type",
      "scope": "source.cs storage.type",
      "settings": {
        "foreground": "#171717"
      }
    },
    {
      "name": "C# return type",
      "scope": "source.cs meta.method.return-type",
      "settings": {
        "foreground": "#171717"
      }
    },
    {
      "name": "C# namespace",
      "scope": "source.cs entity.name.type.namespace",
      "settings": {
        "foreground": "#009cd0"
      }
    }
  ],
  "colors": {
