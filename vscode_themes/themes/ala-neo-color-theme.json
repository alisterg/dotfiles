  "tokenColors": [
    {
      "scope": [
        // "meta.embedded",
        "source.groovy.embedded",
        "string meta.image.inline.markdown"
      ],
      "settings": {
        "foreground": "#E195D8"
      }
    },
    {
      "scope": "string.json.comments",
      "settings": {
        "foreground": "#d9dbe1"
      }
    },
    {
      "scope": ["comment", "comment.block.documentation", "punctuation"],
      "settings": {
        "foreground": "#7D7E7F"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#DDFE4A"
      }
    },
    {
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#DDFE4A99"
      }
    },
    // Regular keywords
    {
      "scope": [
        "storage.type",
        "storage.modifier",
        "variable.language.this",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#9b8de4" // pink ee75ec purp 9b8de4
      }
    },
    // Control flow keywords
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#EE759D" // EE759D
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#E195D8"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#C291F9"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.namespace",
        "entity.name.scope-resolution"
      ],
      "settings": {
        "foreground": "#67FEE4"
      }
    },
    {
      "name": "function definitions",
      "scope": [
        "meta.object.member.ts entity.name.function.ts",
        "meta.method.declaration.ts entity.name.function.ts",
        "meta.definition.function",
        "meta.definition.variable entity.name.function",
        "entity.name.function.cs",
        "meta.function.php"
      ],
      "settings": {
        "foreground": "#C092F4" // blue 6a9eff green A6FD8E
      }
    },
    {
      "name": "overrides for function call",
      "scope": [
        "meta.method.declaration.ts meta.function-call.ts entity.name.function.ts",
        "meta.function-call.ts entity.name.function.ts"
      ],
      "settings": {
        "foreground": "#d9dbe1"
      }
    },
    {
      "name": "tsx overrides",
      "scope": [
        "meta.embedded.expression.tsx",
        "punctuation.section.embedded.begin.tsx",
        "punctuation.section.embedded.end.tsx"
      ],
      "settings": {
        "foreground": "#d9dbe1"
      }
    },
    {
      "scope": [
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end",
        "markup.heading.markdown"
      ],
      "settings": {
        "foreground": "#EE759D"
      }
    },
    {
      "scope": "meta.paragraph.markdown",
      "settings": {
        "foreground": "#d9dbe1"
      }
    },
    {
      "scope": ["constant.language", "meta.preprocessor"],
      "settings": {
        "foreground": "#C291F9"
      }
    },
    {
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#C291F9"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#268BD2"
      }
    },
    {
      "scope": "support.type.primitive",
      "settings": {
        "foreground": "#C291F9"
      }
    },
    {
      "scope": "punctuation.separator.continuation",
      "settings": {
        "foreground": "#DC322F"
      }
    },
    {
      "scope": ["support.constant", "support.variable"],
      "settings": {}
    },
    {
      "scope": "support.type.exception",
      "settings": {
        "foreground": "#CB4B16"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#DC322F"
      }
    },
    {
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#268BD2",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#DC322F",
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#CB4B16",
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#859900"
      }
