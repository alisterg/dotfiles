{
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "colors": {
    "editor.background": "#16161c",
    "editor.foldBackground": "#ece7e700",
    "editor.foreground": "#898d94",
    "editor.lineHighlightBackground": "#1c1c24",
    "editor.selectionBackground": "#6697ac88",
    "editorCursor.foreground": "#898d94",
    "editorIndentGuide.background1": "#dfd5d5",
    "editorLineNumber.activeForeground": "#898d94",
    "editorLineNumber.foreground": "#3b4253",
    "editorWhitespace.foreground": "#3b4253",

    "foreground": "#898d94",

    // UI
    "statusBar.foreground": "#898d94",
    "tab.activeBackground": "#1c1c24",
    "editorGroupHeader.tabsBackground": "#16161c",
    "sideBarTitle.background": "#16161c",
    "sideBar.dropBackground": "#16161c",
    "statusBar.background": "#16161c",
    "tab.border": "#16161c",
    "menu.border": "#16161c",
    "sideBar.border": "#16161c",
    "activityBar.border": "#16161c",
    "activityBar.background": "#16161c",
    "editorWidget.background": "#16161c",
    "panel.border": "#16161c",
    "sideBar.background": "#16161c",
    "titleBar.activeBackground": "#16161c",
    "tab.inactiveBackground": "#16161c",
    "textLink.activeForeground": "#6697ac",
    "editorLink.activeForeground": "#6697ac",
    "list.activeSelectionBackground": "#6697ac88",
    "editor.selectionHighlightBackground": "#6697ac88",
    "editor.wordHighlightBackground": "#6697ac88",
    "editor.findMatchBackground": "#35b8e338",
    "editor.findMatchHighlightBackground": "#9fbc9088",
    "editorGutter.addedBackground": "#35e36038", // 487a1d88
    "editorGutter.deletedBackground": "#7c2b2b",
    "editorGutter.modifiedBackground": "#6f633b88",
    "gitDecoration.addedResourceForeground": "#35e36038",
    "gitDecoration.deletedResourceForeground": "#7c2b2b",
    "gitDecoration.modifiedResourceForeground": "#6f633b88",
    "gitDecoration.ignoredResourceForeground": "#5f6266",
    "list.warningForeground": "#898d94",
    "list.errorForeground": "#8f757c",

    "panel.background": "#0f0f13", // 1e1e26

    // Terminal
    "terminal.ansiBlack": "#16161c",
    "terminal.ansiBrightBlack": "#16161c",
    "terminal.ansiRed": "#b2656b",
    "terminal.ansiBrightRed": "#b2656b",
    "terminal.ansiGreen": "#6697ac",
    "terminal.ansiBrightGreen": "#9fbc90",
    "terminal.ansiYellow": "#d79bbf",
    "terminal.ansiBrightYellow": "#e5cc93",
    "terminal.ansiBlue": "#929fb0",
    "terminal.ansiBrightBlue": "#6697ac",
    "terminal.ansiMagenta": "#e5cb92",
    "terminal.ansiBrightMagenta": "#ae8fa5",
    "terminal.ansiCyan": "#9cb4b3",
    "terminal.ansiBrightCyan": "#9cb4b3",
    "terminal.ansiWhite": "#898d94",
    "terminal.ansiBrightWhite": "#c0c3cc",
    "terminal.foreground": "#898d94",

    // Intellisense icons
    "symbolIcon.classForeground": "#9fbc90",
    "symbolIcon.colorForeground": "#9fbc90",
    "symbolIcon.constructorForeground": "#9fbc90",
    "symbolIcon.enumeratorForeground": "#9fbc90",
    "symbolIcon.interfaceForeground": "#9fbc90",
    "symbolIcon.moduleForeground": "#9fbc90",
    "symbolIcon.namespaceForeground": "#9fbc90",
    "symbolIcon.typeParameterForeground": "#9fbc90",
    "symbolIcon.packageForeground": "#9fbc90",
    "symbolIcon.objectForeground": "#9fbc90",
    "symbolIcon.eventForeground": "#35b8e3",
    "symbolIcon.fileForeground": "#35b8e3",
    "symbolIcon.folderForeground": "#35b8e3",
    "symbolIcon.keywordForeground": "#35b8e3",
    "symbolIcon.operatorForeground": "#35b8e3",
    "symbolIcon.snippetForeground": "#35b8e3",
    "symbolIcon.structForeground": "#35b8e3",
    "symbolIcon.arrayForeground": "#e5cc93",
    "symbolIcon.booleanForeground": "#e5cc93",
    "symbolIcon.methodForeground": "#e5cc93",
    "symbolIcon.propertyForeground": "#e5cc93",
    "symbolIcon.fieldForeground": "#e5cc93",
    "symbolIcon.functionForeground": "#e5cc93",
    "symbolIcon.textForeground": "#65676c",
    "symbolIcon.variableForeground": "#65676c",
    "symbolIcon.stringForeground": "#6697ac",
    "symbolIcon.referenceForeground": "#6697ac",
    "symbolIcon.unitForeground": "#6697ac",
    "symbolIcon.constantForeground": "#6697ac",
    "symbolIcon.enumeratorMemberForeground": "#6697ac",
    "symbolIcon.keyForeground": "#6697ac",
    "symbolIcon.nullForeground": "#6697ac",
    "symbolIcon.numberForeground": "#6697ac"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#3b4253"
      }
    },
    {
      "scope": ["constant", "constant.numeric"],
      "settings": {
        "foreground": "#898d94"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#898d94"
      }
    },
    {
      "scope": ["variable", "variable.parameter"],
      "settings": {
        "foreground": "#898d94"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#e5cc93"
      }
    },
    {
      "scope": [
        "meta.function-call entity.name.function",
        "meta.function-call",
        "variable.function",
        "entity.name.function.support.go",
        "entity.name.type.cs",
        "meta.type.annotation entity.name.type"
      ],
      "settings": {
        "foreground": "#898d94",
        "fontStyle": "none"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage.modifier",
        "punctuation.definition.variable.php"
      ],
      "settings": {
        "foreground": "#c0c3cc"
      }
    },
    {
      "scope": ["storage.type", "support.class.component.tsx", "meta.tag.custom.start.html entity.name.tag.html"],
      "settings": {
        "foreground": "#898d94"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#35b8e3" // 4 2CAF6    // d 79bb f
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#35b8e3"
      }
    },
    {
      "scope": [
        "markup.heading.markdown",
        "support.type.property-name.json",
        "storage.type.class.jsdoc",
        "entity.name.tag.cs",
        "keyword.other.phpdoc.php"
      ],
      "settings": {
        "foreground": "#d79bbf"
      }
    },
    {
      "scope": [],
      "settings": {
        "foreground": "#898d94"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#A50303",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.underline"],
      "settings": {
        "foreground": "#898d94",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "meta.diff",
      "settings": {
        "foreground": "#38551E"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#898d94",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["constant.language", "constant.character"],
      "settings": {
        "foreground": "#898d94"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#38551E"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#554717"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#A50303"
      }
    },
    {
      "scope": "markup.punctuation",
      "settings": {
        "foreground": "#1E5571"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#316BCD"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#CD3131"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#800080"
      }
    }
  ]
}
