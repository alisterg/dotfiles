    "symbolIcon.namespaceForeground": "#FF8F40",
    "symbolIcon.nullForeground": "#39BAE6",
    "symbolIcon.numberForeground": "#39BAE6",
    "symbolIcon.objectForeground": "#FF8F40",
    "symbolIcon.operatorForeground": "#81c8be",
    "symbolIcon.packageForeground": "#FF8F40",
    "symbolIcon.propertyForeground": "#39BAE6",
    "symbolIcon.referenceForeground": "#e5c890",
    "symbolIcon.snippetForeground": "#eebebe",
    "symbolIcon.stringForeground": "#AAD94C",
    "symbolIcon.structForeground": "#EE759D",
    "symbolIcon.textForeground": "#d9dbe1",
    "symbolIcon.typeParameterForeground": "#FF8F40",
    "symbolIcon.unitForeground": "#e5c890",
    "symbolIcon.variableForeground": "#bfbdb6"
  },
  "tokenColors": [
    {
      "scope": [
        "meta.embedded.line.ts",
        "source.groovy.embedded",
        "string meta.image.inline.markdown"
      ],
      "settings": {
        "foreground": "#bfbdb6"
      }
    },
    {
      "scope": "string.json.comments",
      "settings": {
        "foreground": "#bfbdb6"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#6C7986"
      }
    },
    {
      "scope": "comment.block.documentation",
      "settings": {
        "foreground": "#6C7986"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#AAD94C"
      }
    },
    {
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "punctuation.definition.string.template.begin.ts",
        "punctuation.definition.string.template.end.ts"
      ],
      "settings": {
        "foreground": "#AAD94C99"
      }
    },
    {
      "scope": ["keyword.control", "storage.modifier"],
      "settings": {
        "foreground": "#FF8F40"
      }
    },
    {
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.other.using",
        "keyword.other.namespace",
        "keyword.control.from"
      ],
      "settings": {
        "foreground": "#f1646b"
      }
    },
    {
      "scope": ["storage.type", "keyword.other"],
      "settings": {
        "foreground": "#ffb454"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#f76ea9"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#f1646b"
      }
    },
    {
      "scope": "variable.language.this",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.type"],
      "settings": {
        "foreground": "#69c4af"
      }
    },
    {
      "name": "function definitions",
      "scope": [
        "meta.object.member.ts entity.name.function.ts",
        "meta.method.declaration.ts entity.name.function.ts",
        "meta.definition.function",
        "meta.definition.variable entity.name.function",
        "entity.name.function.cs",
        "meta.function.php"
      ],
      "settings": {
        "foreground": "#39BAE6"
      }
    },
    {
      "name": "overrides for function call",
      "scope": [
        "meta.method.declaration.ts meta.function-call.ts entity.name.function.ts",
        "meta.function-call.ts entity.name.function.ts",
        "meta.method-call",
        "meta.function-call.tsx entity.name.function.tsx"
      ],
      "settings": {
        "foreground": "#bfbdb6" // 39BAE6
      }
    },
    {
      "scope": ["punctuation", "meta.brace", "keyword.operator"],
      "settings": {
        "foreground": "#FF8F40bb"
      }
    },
    {
      "name": "tsx overrides",
      "scope": [
        "meta.embedded.expression.tsx",
        "punctuation.section.embedded.begin.tsx",
        "punctuation.section.embedded.end.tsx"
      ],
      "settings": {
        "foreground": "#bfbdb6"
      }
    },
    {
      "scope": [
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end",
        "markup.heading.markdown"
      ],
      "settings": {
        "foreground": "#58CAEF"
      }
    },
    {
      "scope": "meta.paragraph.markdown",
      "settings": {
        "foreground": "#bfbdb6"
      }
    },
    {
      "scope": ["constant.language", "meta.preprocessor"],
      "settings": {
        "foreground": "#f1646b" //cb696d
      }
    },
    {
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#f1646b"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#3990ce"
      }
    },
    {
      "scope": ["support.type.primitive", "keyword.type"],
      "settings": {
        "foreground": "#3990ce"
      }
    },
    {
      "scope": "punctuation.separator.continuation",
      "settings": {
        "foreground": "#DC322F"
      }
    },
    {
      "scope": ["support.constant", "support.variable"],
      "settings": {}
    },
    {
      "scope": "support.type.exception",
      "settings": {
        "foreground": "#CB4B16"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#DC322F"
      }
    },
    {
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#3990ce",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#DC322F",
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#CB4B16",
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "scope": "markup.list",
      "settings": {
        "foreground": "#bfbdb6"
      }
    },
    {
      "scope": ["markup.bold", "markup.italic"],
      "settings": {
        "foreground": "#D33682"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#bfbdb6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#3990ce",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "punctuation.definition.link.title.begin.markdown",
        "punctuation.definition.link.title.end.markdown",
        "string.other.link.title.markdown"
      ],
      "settings": {
        "foreground": "#bfbdb6"
      }
    },
    {
      "scope": "meta.link.inline.markdown",
      "settings": {
        "foreground": "#f76ea9"
      }
    },
    {
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "#3990ce",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "entity.name.tag.html",
        "entity.name.tag.tsx",
        "entity.name.tag.xml"
      ],
      "settings": {
        "foreground": "#67B6A4"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#B267E6"
      }
    }
  ]
}
