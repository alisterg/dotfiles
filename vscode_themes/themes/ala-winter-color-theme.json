{
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "colors": {
    "activityBar.background": "#141415", // 26272c 2d2d2f
    "badge.background": "#047aa6",
    "button.background": "#42454b99",
    "debugExceptionWidget.background": "#00212b",
    "debugExceptionWidget.border": "#ab395b",
    "debugToolBar.background": "#141415",
    "dropdown.background": "#00212b",
    "dropdown.border": "#d7d7d799",
    "editor.background": "#18191c", // 2D3036 202225
    "editor.foreground": "#d1d1d1",
    "editor.lineHighlightBackground": "#141415", // 18191c
    "editorGutter.background": "#141415",
    "sideBar.border": "#757575aa",
    "panel.border": "#757575aa",
    "titleBar.border": "#757575aa",
    "statusBar.border": "#757575aa",
    "editor.selectionBackground": "#757575aa",
    "editor.selectionHighlightBackground": "#757575aa",
    "editor.wordHighlightBackground": "#757575aa",
    "editor.wordHighlightStrongBackground": "#757575aa",
    // "editorError.background": "#d3368277",
    "editorBracketHighlight.foreground1": "#cdcdcd",
    "editorBracketHighlight.foreground2": "#b58900",
    "editorBracketHighlight.foreground3": "#d33682",
    "editorCursor.foreground": "#d7d7d7",
    "editorGroup.border": "#78716a",
    "editorGroup.dropBackground": "#d7d7d744",
    "editorGroupHeader.tabsBackground": "#141415",
    "editorHoverWidget.background": "#141415",
    "editorIndentGuide.activeBackground": "#537d9c80",
    "editorIndentGuide.background": "#646b6b80",
    "editorLineNumber.activeForeground": "#5f5f5f",
    "editorLineNumber.foreground": "#3c3c3c", // 5d5d5d
    "editorMarkerNavigationError.background": "#ab395b",
    "editorMarkerNavigationWarning.background": "#5b7e7a",
    "editorWhitespace.foreground": "#93a1a180",
    "editorWidget.background": "#141415",
    "editorWidget.border": "#141415",
    "errorForeground": "#ffeaea",
    "focusBorder": "#d7d7d799",
    "input.background": "#575a60",
    "input.foreground": "#d7d7d7",
    "input.placeholderForeground": "#93a1a1aa",
    "inputOption.activeBorder": "#2aa19899",
    "inputValidation.errorBackground": "#571b26",
    "inputValidation.errorBorder": "#a92049",
    "inputValidation.infoBackground": "#052730",
    "inputValidation.infoBorder": "#363b5f",
    "inputValidation.warningBackground": "#5d5938",
    "inputValidation.warningBorder": "#9d8a5e",
    "list.activeSelectionBackground": "#393d44",
    "list.dropBackground": "#00445488",
    "list.highlightForeground": "#1ebcc5",
    "list.hoverBackground": "#5b5e64aa",
    "list.inactiveSelectionBackground": "#18191c",
    "minimap.selectionHighlight": "#274642",
    // "panel.border": "#424242",
    "peekView.border": "#d7d7d799",
    "peekViewEditor.background": "#10192c",
    "peekViewEditor.matchHighlightBackground": "#7744aa40",
    "peekViewResult.background": "#00212b",
    "peekViewTitle.background": "#00212b",
    "pickerGroup.border": "#424242",
    "pickerGroup.foreground": "#d1d1d1",
    "ports.iconRunningProcessForeground": "#369432",
    "progressBar.background": "#047aa6",
    "quickInputList.focusBackground": "#575a60",
    "quickInput.background": "#272c38",
    "selection.background": "#d7d7d799",
    "sideBar.background": "#141415",
    "sideBarTitle.foreground": "#93a1a1",
    "sideBarSectionHeader.background": "#18191c",
    "statusBar.background": "#141415",
    "statusBar.debuggingBackground": "#00212b",
    "statusBar.foreground": "#93a1a1",
    "statusBar.noFolderBackground": "#00212b",
    "statusBarItem.prominentBackground": "#003847",
    "statusBarItem.prominentHoverBackground": "#003847",
    "statusBarItem.remoteBackground": "#2aa19899",
    "tab.activeBackground": "#303237",
    "tab.activeForeground": "#d6dbdb",
    // "tab.border": "#003847",
    "tab.inactiveBackground": "#141415",
    "tab.inactiveForeground": "#93a1a1",
    "tab.lastPinnedBorder": "#2aa19844",
    "terminal.ansiBlack": "#073642",
    "terminal.ansiBlue": "#268bd2",
    "terminal.ansiBrightBlack": "#002b36",
    "terminal.ansiBrightBlue": "#839496",
    "terminal.ansiBrightCyan": "#93a1a1",
    "terminal.ansiBrightGreen": "#586e75",
    "terminal.ansiBrightMagenta": "#6c71c4",
    "terminal.ansiBrightRed": "#cb4b16",
    "terminal.ansiBrightWhite": "#fdf6e3",
    "terminal.ansiBrightYellow": "#657b83",
    "terminal.ansiCyan": "#2aa198",
    "terminal.ansiGreen": "#859900",
    "terminal.ansiMagenta": "#d33682",
    "terminal.ansiRed": "#dc322f",
    "terminal.ansiWhite": "#eee8d5",
    "terminal.ansiYellow": "#b58900",
    "titleBar.activeBackground": "#141415",
    "editorGutter.addedBackground": "#0e4d32",
    "editorGutter.deletedBackground": "#772828",
    "editorGutter.modifiedBackground": "#18576f",
    "editorHoverWidget.border": "#b7b7b7",
    "notifications.background": "#141415",
    "panel.background": "#141415"
  },
  "tokenColors": [
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded",
        "string meta.image.inline.markdown",
        "meta.interpolation"
      ],
      "settings": {
        "foreground": "#d0afa2"
      }
    },
    {
      "scope": "string.json.comments",
      "settings": {
        "foreground": "#d1d1d1"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#867f7f" // foreground": "#c8344d"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier.async",
        "storage.modifier.cs",
        "storage.modifier.ts"
      ],
      "settings": {
        "foreground": "#43b3e2",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant", "support.type.primitive", "keyword.type.cs"],
      "settings": {
        "foreground": "#5BC9E1", //  a780da
        "fontStyle": "none"
      }
    },
    // {
    // 	"scope": "keyword.control.import",
    // 	"settings": {
    // 		"foreground": "#73cfac"
    // 	}
    // },
    {
      "scope": "comment.block.documentation",
      "settings": {
        "foreground": "#777"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#E195D8"
      }
    },
    {
      "scope": "variable.language.this",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.type.namespace"],
      "settings": {
        "foreground": "#deb8e1"
      }
    },
    {
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "entity.name.namespace",
        "entity.name.scope-resolution"
      ],
      "settings": {
        "foreground": "#d899dd"
      }
    },
    {
      "name": "function definitions",
      "scope": [
        "meta.object.member.ts entity.name.function.ts",
        "meta.method.declaration.ts entity.name.function.ts",
        "meta.definition.function",
        "meta.definition.variable entity.name.function",
        "entity.name.function.cs"
      ],
      "settings": {
        "foreground": "#DCDCAA" // DCDCAA
      }
    },
    {
      "name": "overrides for function call",
      "scope": [
        "meta.method.declaration.ts meta.function-call.ts entity.name.function.ts",
        "meta.function-call.ts entity.name.function.ts"
      ],
      "settings": {
        "foreground": "#d1d1d1"
      }
    },
    {
      "scope": ["keyword.preprocessor", "meta.preprocessor"],
      "settings": {
        "fontStyle": "bold underline"
      }
    },
    {
      "name": "tsx overrides",
      "scope": [
        "meta.embedded.expression.tsx",
        "punctuation.section.embedded.begin.tsx",
        "punctuation.section.embedded.end.tsx"
      ],
      "settings": {
        "foreground": "#d1d1d1"
      }
    },
    {
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#d1d1d1"
      }
    },
    {
      "scope": [
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end",
        "markup.heading.markdown"
      ],
      "settings": {
        "foreground": "#94C741"
      }
    },
    {
      "scope": "meta.paragraph.markdown",
      "settings": {
        "foreground": "#d1d1d1"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#268BD2"
      }
    },
    {
      "scope": "punctuation.separator.continuation",
      "settings": {
        "foreground": "#DC322F"
      }
    },
    {
      "scope": "support.type.exception",
      "settings": {
        "foreground": "#CB4B16"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#DC322F"
      }
    },
    {
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#268BD2",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#DC322F",
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#CB4B16",
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "scope": "markup.list",
      "settings": {
        "foreground": "#d1d1d1"
      }
    },
    {
      "scope": ["markup.bold", "markup.italic"],
      "settings": {
        "foreground": "#D33682"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#d1d1d1",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#268BD2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "punctuation.definition.link.title.begin.markdown",
        "punctuation.definition.link.title.end.markdown",
        "string.other.link.title.markdown"
      ],
      "settings": {
        "foreground": "#d1d1d1"
      }
    },
    {
      "scope": "meta.link.inline.markdown",
      "settings": {
        "foreground": "#E195D8"
      }
    },
    {
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "#268BD2",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "entity.name.tag.html",
        "entity.name.tag.tsx",
        "entity.name.tag.xml"
      ],
      "settings": {
        "foreground": "#4686c7" // 89C2FB
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#43b3e2"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#B267E6"
      }
    }
  ]
}
