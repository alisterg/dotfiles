		"editorBracketHighlight.foreground2": "#b58900",
		"editorBracketHighlight.foreground3": "#d33682",
		"editorCursor.foreground": "#d7d7d7",
		"editorGroup.border": "#df9648",
		"editorGroup.dropBackground": "#d7d7d744",
		"editorGroupHeader.tabsBackground": "#2F3036",
		"editorHoverWidget.background": "#2F3036",
		"editorIndentGuide.activeBackground": "#454758",
		"editorIndentGuide.background": "#393a43",
		"editorLineNumber.activeForeground": "#949494",
		"editorLineNumber.foreground": "#5d5d5d",
		"editorMarkerNavigationError.background": "#ab395b",
		"editorMarkerNavigationWarning.background": "#5b7e7a",
		"editorWhitespace.foreground": "#93a1a180",
		"editorWidget.background": "#2F3036",
		"editorWidget.border": "#2F3036",
		"errorForeground": "#ffeaea",
		"focusBorder": "#d7d7d799",
		"input.background": "#575a60",
		"input.foreground": "#d7d7d7",
		"input.placeholderForeground": "#93a1a1aa",
		"inputOption.activeBorder": "#2aa19899",
		"inputValidation.errorBackground": "#571b26",
		"inputValidation.errorBorder": "#a92049",
		"inputValidation.infoBackground": "#052730",
		"inputValidation.infoBorder": "#363b5f",
		"inputValidation.warningBackground": "#5d5938",
		"inputValidation.warningBorder": "#9d8a5e",
		"list.activeSelectionBackground": "#393d44",
		"list.dropBackground": "#00445488",
		"list.highlightForeground": "#1ebcc5",
		"list.hoverBackground": "#5b5e64aa",
		"list.inactiveSelectionBackground": "#292A30",
		"list.errorForeground": "#bb726e",
		"list.warningForeground": "#D7D7D9",
		"minimap.selectionHighlight": "#274642",
		// "panel.border": "#424242",
		"peekView.border": "#d7d7d799",
		"peekViewEditor.background": "#10192c",
		"peekViewEditor.matchHighlightBackground": "#7744aa40",
		"peekViewResult.background": "#00212b",
		"peekViewTitle.background": "#00212b",
		"pickerGroup.border": "#424242",
		"pickerGroup.foreground": "#D7D7D9",
		"ports.iconRunningProcessForeground": "#369432",
		"progressBar.background": "#047aa6",
		"quickInputList.focusBackground": "#575a60",
		"quickInput.background": "#2F3036",
		"selection.background": "#d7d7d799",
		"sideBar.background": "#2F3036",
		"sideBarTitle.foreground": "#93a1a1",
		"sideBarSectionHeader.background": "#292A30",
		"statusBar.background": "#2F3036",
		"statusBar.debuggingBackground": "#00212b",
		"statusBar.foreground": "#93a1a1",
		"statusBar.noFolderBackground": "#00212b",
		"statusBarItem.prominentBackground": "#003847",
		"statusBarItem.prominentHoverBackground": "#003847",
		"statusBarItem.remoteBackground": "#2aa19899",
		"tab.activeBackground": "#292A30",
		"tab.activeForeground": "#d6dbdb",
		// "tab.border": "#003847",
		"tab.inactiveBackground": "#2F3036",
		"tab.inactiveForeground": "#93a1a1",
		"tab.lastPinnedBorder": "#2aa19844",
		"terminal.ansiBlack": "#073642",
		"terminal.ansiBlue": "#268bd2",
		"terminal.ansiBrightBlack": "#002b36",
		"terminal.ansiBrightBlue": "#839496",
		"terminal.ansiBrightCyan": "#93a1a1",
		"terminal.ansiBrightGreen": "#586e75",
		"terminal.ansiBrightMagenta": "#6c71c4",
		"terminal.ansiBrightRed": "#cb4b16",
		"terminal.ansiBrightWhite": "#fdf6e3",
		"terminal.ansiBrightYellow": "#657b83",
		"terminal.ansiCyan": "#2aa198",
		"terminal.ansiGreen": "#859900",
		"terminal.ansiMagenta": "#d33682",
		"terminal.ansiRed": "#dc322f",
		"terminal.ansiWhite": "#eee8d5",
		"terminal.ansiYellow": "#b58900",
		"titleBar.activeBackground": "#2F3036",
		"editorGutter.addedBackground": "#0e4d32",
		"editorGutter.deletedBackground": "#772828",
		"editorGutter.modifiedBackground": "#18576f",
		"editorHoverWidget.border": "#b7b7b7",
		"notifications.background": "#2F3036",
		"panel.background": "#2F3036",
	},
	"tokenColors": [
		{
			"scope": [
				// "meta.embedded",
				"source.groovy.embedded",
				"string meta.image.inline.markdown"
			],
			"settings": {
				"foreground": "#f76ea9"
			}
		},
		{
			"scope": "string.json.comments",
			"settings": {
				"foreground": "#D7D7D9"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#6C7986"
			}
		},
		{
			"scope": "comment.block.documentation",
			"settings": {
				"foreground": "#6C7986"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#f47b70"
			}
		},
		{
			"scope": ["keyword.control", "storage.modifier"],
			"settings": {
				"foreground": "#f76ea9"
			}
		},
		{
			"scope": ["variable"],
			"settings": {
				"foreground": "#D0A8FF"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#f76ea9"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#C8B766"
			}
		},
		{
			"scope": "variable.language.this",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"entity.name.type",
				"entity.name.type.interface",
				"entity.name.type.class",
				"entity.name.type.alias.ts",
				"entity.name.namespace",
				"entity.name.type.namespace",
				"entity.name.scope-resolution",
				"entity.name.type.enum"
			],
			"settings": {
				"foreground": "#58CAEF"
			}
		},
		{
			"name": "function definitions",
			"scope": [
				"meta.object.member.ts entity.name.function.ts",
				"meta.method.declaration.ts entity.name.function.ts",
				"meta.definition.function",
				"meta.definition.variable entity.name.function",
				"entity.name.function.cs",
				"meta.function.php"
			],
			"settings": {
				"foreground": "#41A2C0"
			}
		},
		{
			"name": "overrides for function call",
			"scope": [
				"meta.method.declaration.ts meta.function-call.ts entity.name.function.ts",
				"meta.function-call.ts entity.name.function.ts",
				"meta.method-call"
			],
			"settings": {
				"foreground": "#A167E6"
			}

		},
		{
			"name": "tsx overrides",
			"scope": [
				"meta.embedded.expression.tsx",
				"punctuation.section.embedded.begin.tsx",
				"punctuation.section.embedded.end.tsx"
			],
			"settings": {
				"foreground": "#D7D7D9"
			}
		},
		{
			"scope": [
				"punctuation.section.embedded.begin",
				"punctuation.section.embedded.end",
				"markup.heading.markdown"
			],
			"settings": {
				"foreground": "#58CAEF"
			}
		},
		{
			"scope": "meta.paragraph.markdown",
			"settings": {
				"foreground": "#D7D7D9"
			}
		},
		{
			"scope": [
				"constant.language",
				"meta.preprocessor"
			],
			"settings": {
				"foreground": "#C8B766"
			}
		},
		{
			"scope": [
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#C8B766"
			}
		},
		{
			"scope": "support.function",
			"settings": {
				"foreground": "#268BD2"
			}
		},
		{
			"scope": "support.type.primitive",
			"settings": {
				"foreground": "#C8B766"
			}
		},
		{
			"scope": "punctuation.separator.continuation",
			"settings": {
				"foreground": "#DC322F"
			}
		},
		{
			"scope": [
				"support.constant",
				"support.variable"
			],
			"settings": {}
		},
		{
			"scope": "support.type.exception",
			"settings": {
				"foreground": "#CB4B16"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#DC322F"
			}
		},
		{
			"scope": [
				"meta.diff",
				"meta.diff.header"
			],
			"settings": {
				"foreground": "#268BD2",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#DC322F",
				"fontStyle": ""
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#CB4B16",
				"fontStyle": ""
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#859900"
			}
		},
		{
			"scope": "markup.quote",
			"settings": {
				"foreground": "#859900"
			}
		},
		{
			"scope": "markup.list",
			"settings": {
				"foreground": "#D7D7D9"
			}
		},
		{
			"scope": [
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "#D33682"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"foreground": "#D7D7D9",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.strikethrough",
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#E6DB74"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"foreground": "#268BD2",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"punctuation.definition.link.title.begin.markdown",
				"punctuation.definition.link.title.end.markdown",
				"string.other.link.title.markdown"
			],
			"settings": {
				"foreground": "#D7D7D9"
			}
		},
		{
			"scope": "meta.link.inline.markdown",
			"settings": {
				"foreground": "#f76ea9"
			}
		},
		{
			"scope": "markup.heading.setext",
			"settings": {
				"foreground": "#268BD2",
				"fontStyle": ""
			}
		},
		{
			"scope": ["entity.name.tag.html", "entity.name.tag.tsx", "entity.name.tag.xml"],
			"settings": {
				"foreground": "#67B6A4"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796E6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#CD9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#F44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#B267E6"
			}
		}
	]
}