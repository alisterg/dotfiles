    "statusBar.background": "#f0f3f4",
    "statusBar.debuggingBackground": "#f0f3f4",
    "statusBar.foreground": "#393f3f",
    "statusBar.noFolderBackground": "#f0f3f4",
    "statusBarItem.prominentBackground": "#003847",
    "statusBarItem.prominentHoverBackground": "#003847",
    "statusBarItem.remoteBackground": "#2aa19899",
