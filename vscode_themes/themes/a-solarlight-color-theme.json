    "editorBracketHighlight.foreground2": "#e83b8ebb",
    "editorBracketHighlight.foreground3": "#268bd2bb",
    "editorBracketHighlight.foreground1": "#2aa198bb",
    "editorBracketHighlight.foreground4": "#cb4b16bb",
    // icons
    "symbolIcon.arrayForeground": "#268bd2",
    "symbolIcon.booleanForeground": "#268bd2",
    "symbolIcon.classForeground": "#e83b8e",
    "symbolIcon.colorForeground": "#e83b8e",
    "symbolIcon.constantForeground": "#ef9f76",
    "symbolIcon.constructorForeground": "#e83b8e",
    "symbolIcon.enumeratorForeground": "#e83b8e",
    "symbolIcon.enumeratorMemberForeground": "#e5c890",
    "symbolIcon.eventForeground": "#f4b8e4",
    "symbolIcon.fieldForeground": "#268bd2",
    "symbolIcon.fileForeground": "#85c1dc",
    "symbolIcon.folderForeground": "#85c1dc",
    "symbolIcon.functionForeground": "#268bd2",
    "symbolIcon.interfaceForeground": "#e83b8e",
    "symbolIcon.keyForeground": "#2aa198",
    "symbolIcon.keywordForeground": "#e83b8e",
    "symbolIcon.methodForeground": "#268bd2",
    "symbolIcon.moduleForeground": "#e83b8e",
    "symbolIcon.namespaceForeground": "#e83b8e",
    "symbolIcon.nullForeground": "#268bd2",
    "symbolIcon.numberForeground": "#268bd2",
    "symbolIcon.objectForeground": "#e83b8e",
    "symbolIcon.operatorForeground": "#2aa198",
    "symbolIcon.packageForeground": "#e83b8e",
    "symbolIcon.propertyForeground": "#268bd2",
    "symbolIcon.referenceForeground": "#e5c890",
    "symbolIcon.snippetForeground": "#eebebe",
    "symbolIcon.stringForeground": "#2aa198",
    "symbolIcon.structForeground": "#EE759D",
    "symbolIcon.textForeground": "#d9dbe1",
    "symbolIcon.typeParameterForeground": "#e83b8e",
    "symbolIcon.unitForeground": "#e5c890",
    "symbolIcon.variableForeground": "#393f3f"
  },
  "tokenColors": [
    {
      "scope": [
        "meta.embedded.line.ts",
        "source.groovy.embedded",
        "string meta.image.inline.markdown"
      ],
      "settings": {
        "foreground": "#393f3f"
      }
    },
    {
      "scope": [
        "string.json.comments",
        "support.type.property-name.json",
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#393f3f"
      }
    },
    {
      "scope": "punctuation.definition.comment",
      "settings": {
        "foreground": "#2aa19899"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#93a1a1"
      }
    },
    {
      "scope": "comment.block.documentation",
      "settings": {
        "foreground": "#93a1a1"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#2aa198"
      }
    },
    {
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "punctuation.definition.string.template.begin.ts",
        "punctuation.definition.string.template.end.ts"
      ],
      "settings": {
        "foreground": "#2aa19899"
      }
    },
    {
      "scope": [
        "keyword.control",
        "storage.modifier",
        "keyword.other",
        "storage.type"
      ],
      "settings": {
        // "foreground": "#e83b8e"
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.other.using",
        "keyword.other.namespace",
        "keyword.control.from",
        "keyword.other.use.php"
      ],
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#f76ea9"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#92b300"
      }
    },
    {
      "scope": "variable.language.this",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        // "variable.parameter",
        "punctuation.definition.variable.php",
      ],
      "settings": {
        "foreground": "#92b300"
      }
    },
    {
      "scope": ["entity.name.type"],
      "settings": {
        "foreground": "#6c71c4"
      }
    },
    {
      "name": "function definitions",
      "scope": [
        "meta.object.member.ts entity.name.function.ts",
        "meta.method.declaration.ts entity.name.function.ts",
        "meta.definition.function",
        "meta.definition.variable entity.name.function"
      ],
      "settings": {
        "foreground": "#393f3f"
      }
    },
    {
      "name": "overrides for function call",
      "scope": [
        "meta.method.declaration.ts meta.function-call.ts entity.name.function.ts",
        "meta.function-call.ts entity.name.function.ts",
        "meta.method-call",
        "entity.name.function.cs",
        "meta.function.php",
        "meta.function-call.tsx entity.name.function.tsx"
      ],
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "name": "tsx overrides",
      "scope": [
        "meta.embedded.expression.tsx",
        "punctuation.section.embedded.begin.tsx",
        "punctuation.section.embedded.end.tsx"
      ],
      "settings": {
        "foreground": "#393f3f"
      }
    },
    {
      "scope": [
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end",
        "markup.heading.markdown"
      ],
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "scope": "meta.paragraph.markdown",
      "settings": {
        "foreground": "#393f3f"
      }
    },
    {
      "scope": ["meta.preprocessor"],
      "settings": {
        "foreground": "#cb4b16" //cb696d
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#6c71c4"
      }
    },
    {
      "scope": ["support.type.primitive", "keyword.type"],
      "settings": {
        "foreground": "#6c71c4"
      }
    },
    {
      "scope": "punctuation.separator.continuation",
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "scope": ["support.constant", "support.variable"],
      "settings": {}
    },
    {
      "scope": "support.type.exception",
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#6c71c4",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#cb4b16",
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#cb4b16",
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "scope": "markup.list",
      "settings": {
        "foreground": "#393f3f"
      }
    },
    {
      "scope": ["markup.bold", "markup.italic"],
      "settings": {
        "foreground": "#e83b8e"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#393f3f",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#2AA198"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#6c71c4",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "punctuation.definition.link.title.begin.markdown",
        "punctuation.definition.link.title.end.markdown"
      ],
      "settings": {
        "foreground": "#93a1a1"
      }
    },
    {
      "scope": "meta.link.inline.markdown",
      "settings": {
        "foreground": "#f76ea9"
      }
    },
    {
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "#6c71c4",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "entity.name.tag.html",
        "entity.name.tag.tsx",
        "entity.name.tag.xml"
      ],
      "settings": {
        "foreground": "#6c71c4"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#B267E6"
      }
    }
  ]
}
