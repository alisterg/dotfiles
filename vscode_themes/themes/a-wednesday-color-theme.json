    "input.background": "#575a60",
    "input.foreground": "#d7d7d7",
    "input.placeholderForeground": "#93a1a1aa",
    "inputOption.activeBorder": "#2aa19899",
    "inputValidation.errorBackground": "#571b26",
    "inputValidation.errorBorder": "#a92049",
    "inputValidation.infoBackground": "#052730",
    "inputValidation.infoBorder": "#363b5f",
    "inputValidation.warningBackground": "#5d5938",
    "inputValidation.warningBorder": "#9d8a5e",
    "list.activeSelectionBackground": "#393d44",
    "list.dropBackground": "#00445488",
    "list.highlightForeground": "#1ebcc5",
    "list.hoverBackground": "#5b5e64aa",
    "list.inactiveSelectionBackground": "#262626",
    "list.errorForeground": "#bb726e",
    "list.warningForeground": "#BDBDBD",
    "minimap.selectionHighlight": "#274642",
    // "panel.border": "#424242",
    "peekView.border": "#d7d7d799",
    "peekViewEditor.background": "#10192c",
    "peekViewEditor.matchHighlightBackground": "#7744aa40",
    "peekViewResult.background": "#00212b",
    "peekViewTitle.background": "#00212b",
    "pickerGroup.border": "#424242",
    "pickerGroup.foreground": "#BDBDBD",
    "ports.iconRunningProcessForeground": "#369432",
    "progressBar.background": "#047aa6",
    "quickInputList.focusBackground": "#575a60",
    "quickInput.background": "#313235",
    "selection.background": "#d7d7d799",
    "sideBar.background": "#313235",
    "sideBarTitle.foreground": "#93a1a1",
    "sideBarSectionHeader.background": "#262626",
    "statusBar.background": "#313235",
    "statusBar.debuggingBackground": "#00212b",
    "statusBar.foreground": "#93a1a1",
    "statusBar.noFolderBackground": "#00212b",
    "statusBarItem.prominentBackground": "#003847",
    "statusBarItem.prominentHoverBackground": "#003847",
    "statusBarItem.remoteBackground": "#2aa19899",
    "tab.activeBackground": "#262626",
    "tab.activeForeground": "#d6dbdb",
    // "tab.border": "#003847",
    "tab.inactiveBackground": "#313235",
    "tab.inactiveForeground": "#93a1a1",
    "tab.lastPinnedBorder": "#2aa19844",
    "terminal.ansiBlack": "#073642",
    "terminal.ansiBlue": "#268bd2",
    "terminal.ansiBrightBlack": "#002b36",
    "terminal.ansiBrightBlue": "#839496",
    "terminal.ansiBrightCyan": "#93a1a1",
    "terminal.ansiBrightGreen": "#586e75",
    "terminal.ansiBrightMagenta": "#6c71c4",
    "terminal.ansiBrightRed": "#cb4b16",
    "terminal.ansiBrightWhite": "#fdf6e3",
    "terminal.ansiBrightYellow": "#657b83",
    "terminal.ansiCyan": "#2aa198",
    "terminal.ansiGreen": "#859900",
    "terminal.ansiMagenta": "#d33682",
    "terminal.ansiRed": "#dc322f",
    "terminal.ansiWhite": "#eee8d5",
    "terminal.ansiYellow": "#b58900",
    "titleBar.activeBackground": "#313235",
    "editorGutter.addedBackground": "#85990050",
    "editorGutter.deletedBackground": "#ff434050",
    "editorGutter.modifiedBackground": "#535664",
    "editorHoverWidget.border": "#b7b7b7",
    "notifications.background": "#313235",
    "panel.background": "#313235",
    // icons
    "symbolIcon.arrayForeground": "#B5CB75",
    "symbolIcon.booleanForeground": "#B5CB75",
    "symbolIcon.classForeground": "#C191FF",
    "symbolIcon.colorForeground": "#C191FF",
    "symbolIcon.constantForeground": "#ef9f76",
    "symbolIcon.constructorForeground": "#C191FF",
    "symbolIcon.enumeratorForeground": "#C191FF",
    "symbolIcon.enumeratorMemberForeground": "#e5c890",
    "symbolIcon.eventForeground": "#f4b8e4",
    "symbolIcon.fieldForeground": "#B5CB75",
    "symbolIcon.fileForeground": "#85c1dc",
    "symbolIcon.folderForeground": "#85c1dc",
    "symbolIcon.functionForeground": "#B5CB75",
    "symbolIcon.interfaceForeground": "#C191FF",
    "symbolIcon.keyForeground": "#28bdae",
    "symbolIcon.keywordForeground": "#6C95EB",
    "symbolIcon.methodForeground": "#B5CB75",
    "symbolIcon.moduleForeground": "#C191FF",
    "symbolIcon.namespaceForeground": "#C191FF",
    "symbolIcon.nullForeground": "#e5c890",
    "symbolIcon.numberForeground": "#e5c890",
    "symbolIcon.objectForeground": "#C191FF",
    "symbolIcon.operatorForeground": "#6C95EB",
    "symbolIcon.packageForeground": "#C191FF",
    "symbolIcon.propertyForeground": "#B5CB75",
    "symbolIcon.referenceForeground": "#e5c890",
    "symbolIcon.snippetForeground": "#eebebe",
    "symbolIcon.stringForeground": "#28bdae",
    "symbolIcon.structForeground": "#EE759D",
    "symbolIcon.textForeground": "#d9dbe1",
    "symbolIcon.typeParameterForeground": "#C191FF",
    "symbolIcon.unitForeground": "#e5c890",
    "symbolIcon.variableForeground": "#b6cccc"
  },
  "tokenColors": [
    {
      "scope": [
        // "meta.embedded",
        "source.groovy.embedded",
        "string meta.image.inline.markdown"
      ],
      "settings": {
        "foreground": "#6C95EB"
      }
    },
    {
      "scope": "string.json.comments",
      "settings": {
        "foreground": "#BDBDBD"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#85C46C"
      }
    },
    {
      "scope": "comment.block.documentation",
      "settings": {
        "foreground": "#85C46C"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#C9A26D"
      }
    },
    {
      "scope": ["keyword.control", "storage.modifier", "keyword.other.await"],
      "settings": {
        "foreground": "#6C95EB"
      }
    },
    {
      "scope": [
        "storage.type",
        "keyword.other.var.cs",
        "keyword.other.new",
        "keyword.function.go",
        "keyword.import.go"
      ],
      "settings": {
        "foreground": "#8795b2"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#D688D4"
      }
    },
    {
      "scope": ["constant.numeric", "punctuation.definition.variable.php"],
      "settings": {
        "foreground": "#ED94C0"
      }
    },
    {
      "scope": "variable.language.this",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.type.class",
        "entity.name.type.alias.ts",
        "entity.name.scope-resolution",
        "entity.name.type.enum",
        "entity.name.type.interface",
        "entity.other.inherited-class.ts",
        "support.class.php"
      ],
      "settings": {
        "foreground": "#C191FF"
      }
    },
    {
      "scope": [
        "entity.name.type.namespace.cs",
        "entity.name.type.interface.cs",
        "entity.name.namespace",
        "entity.name.type.enum"
      ],
      "settings": {
        "foreground": "#E1BFFF"
      }
    },
    {
      "name": "function definitions",
      "scope": [
        "meta.object.member.ts entity.name.function.ts",
        "meta.method.declaration.ts entity.name.function.ts",
        "meta.definition.function",
        "meta.definition.variable entity.name.function",
        "entity.name.function.cs",
        "entity.name.function.go",
        "meta.function.php",
        "meta.definition.method.js entity.name.function.js"
      ],
      "settings": {
        "foreground": "#39CC8F"
      }
    },
    {
      "name": "overrides for function call",
      "scope": [
        "meta.method.declaration.ts meta.function-call.ts entity.name.function.ts",
        "meta.function-call.ts entity.name.function.ts",
        "meta.method-call",
        "variable.other.object.property.cs",
        "meta.function-call.js entity.name.function.js",
        "support.function.go"
      ],
      "settings": {
        "foreground": "#66C3CC"
      }
    },
    {
      "scope": [
        "entity.name.variable.parameter.cs",
        "entity.name.variable.local.cs",
        "meta.object-literal.key",
        "support.variable.property.ts"
      ],
      "settings": {
        "foreground": "#B5CB75"
      }
    },
    {
      "name": "tsx overrides",
      "scope": [
        "meta.embedded.expression.tsx",
        "punctuation.section.embedded.begin.tsx",
        "punctuation.section.embedded.end.tsx",
        "storage.type.class.jsdoc",
        "punctuation.separator.key-value.ts"
      ],
      "settings": {
        "foreground": "#BDBDBD"
      }
    },
    {
      "scope": ["punctuation.section.embedded.begin", "punctuation.section.embedded.end", "markup.heading.markdown"],
      "settings": {
        "foreground": "#C191FF"
      }
    },
    {
      "scope": "meta.paragraph.markdown",
      "settings": {
        "foreground": "#BDBDBD"
      }
    },
    {
      "scope": ["constant.language", "meta.preprocessor"],
      "settings": {
        "foreground": "#ED94C0"
      }
    },
    {
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#ED94C0"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#268BD2"
      }
    },
    {
      "scope": ["support.type.primitive", "keyword.type.cs"],
      "settings": {
        "foreground": "#ED94C0"
      }
    },
    {
      "scope": "punctuation.separator.continuation",
      "settings": {
        "foreground": "#DC322F"
      }
    },
    {
      "scope": ["support.constant", "support.variable"],
      "settings": {}
    },
    {
      "scope": "support.type.exception",
      "settings": {
        "foreground": "#CB4B16"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#DC322F"
      }
    },
    {
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#268BD2",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#DC322F",
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#CB4B16",
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "scope": "markup.list",
      "settings": {
        "foreground": "#BDBDBD"
      }
    },
    {
      "scope": ["markup.bold", "markup.italic"],
      "settings": {
        "foreground": "#D33682"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#BDBDBD",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#268BD2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "punctuation.definition.link.title.begin.markdown",
        "punctuation.definition.link.title.end.markdown",
        "string.other.link.title.markdown"
      ],
      "settings": {
        "foreground": "#BDBDBD"
      }
    },
    {
      "scope": "meta.link.inline.markdown",
      "settings": {
        "foreground": "#6C95EB"
      }
    },
    {
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "#268BD2",
        "fontStyle": ""
      }
    },
    {
      "scope": ["entity.name.tag.cs", "variable.other.jsdoc"],
      "settings": {
        "foreground": "#487D34"
      }
    },
    {
      "scope": ["entity.name.tag.html", "entity.name.tag.tsx", "entity.name.tag.xml"],
      "settings": {
        "foreground": "#C191FF"
      }
    },
    {
      "scope": "meta.tag.other.unrecognized.html.derivative entity.name.tag.html",
      "settings": {
        "foreground": "#26BDA4"
      }
    },
    {
      "scope": "entity.other.attribute-name.html",
      "settings": {
        "foreground": "#85C46C"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#B267E6"
      }
    }
  ]
}
